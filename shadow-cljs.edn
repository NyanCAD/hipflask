;; shadow-cljs configuration
{:source-paths
 ["src/dev"
  "src/main"
  "src/test"]

 :dependencies
 []

 :dev-http {8080 "public"}

 :builds
{:app {:target :browser
       :output-dir "public/js"
       :asset-path "/js"
       :modules {:main {:entries [nyancad.hipflask]}}}
 :test
 {:target    :node-test
  :output-to "node-tests.js"
  :ns-regexp "-test$"
  :autorun   true}}}